  "version": "0.2.0",
  "configurations": [
    {
      "request": "launch",
      "name": "Launch Program",
      "type": "node",
      "program": "app.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "/usr/local/bin/deno",
      "runtimeArgs": ["run", "--inspect-wait", "--allow-all", "--watch"],
      "attachSimplePort": 9229
    }
  ]
}
